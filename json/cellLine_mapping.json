{
    "cellLine": {
        "_all": {"enabled": false},
        "properties": {
            "searchable": {
                "properties": {
                    "_other": {
                        "type": "string",
                        "index": "analyzed",
                        "index_analyzer": "lowercase_whitespace",
                        "search_analyzer": "hipsci_words"
                    },
                    "hipsciShortName": {
                        "type": "string",
                        "index": "analyzed",
                        "index_analyzer": "hipsci_long_to_short",
                        "search_analyzer": "find_short_name"
                    },
                    "hipsciFullName": {
                        "type": "string",
                        "index": "analyzed",
                        "analyzer": "hipsci_long_name"
                    },
                    "prefixes": {
                        "type": "string",
                        "index": "analyzed",
                        "analyzer": "prefix_only",
                        "search_analyzer": "max_length"
                    }
                }
            },
            "assays": {
                "properties": {
                    "count": {
                        "type": "integer"
                    },
                    "cellbiol-fn": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "path": {
                                "type": "string",
                                "index": "not_analyzed"
                            }
                        }
                    },
                    "exomeseq": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "study": {
                                "type": "string",
                                "index": "not_analyzed",
                                "copy_to": "searchable._other"
                            }
                        }
                    },
                    "gexarray": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "study": {
                                "type": "string",
                                "index": "not_analyzed",
                                "copy_to": "searchable._other"
                            }
                        }
                    },
                    "gtarray": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "study": {
                                "type": "string",
                                "index": "not_analyzed",
                                "copy_to": "searchable._other"
                            }
                        }
                    },
                    "mtarray": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "study": {
                                "type": "string",
                                "index": "not_analyzed",
                                "copy_to": "searchable._other"
                            }
                        }
                    },
                    "proteomics": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "path": {
                                "type": "string",
                                "index": "not_analyzed"
                            }
                        }
                    },
                    "rnaseq": {
                        "properties": {
                            "archive": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "study": {
                                "type": "string",
                                "index": "not_analyzed",
                                "copy_to": "searchable._other"
                            }
                        }
                    }
                }
            },
            "bankingStatus": {
                "type": "string",
                "index": "not_analyzed"
            },
            "bioSamplesAccession": {
                "type": "string",
                "index": "not_analyzed",
                "copy_to": "searchable._other"
            },
            "cnv": {
                "properties": {
                    "aberrant_images": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "length_different_regions_Mbp": {
                        "type": "float"
                    },
                    "length_shared_differences": {
                        "type": "float"
                    },
                    "num_different_regions": {
                        "type": "integer"
                    }
                }
            },
            "dateOfDerivation": {
                "format": "dateOptionalTime",
